{
  "name": "@cursorversinc/guidescope",
  "version": "1.0.0",
  "description": "Japanese medical guideline search prompt generator",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "medical",
    "guideline",
    "japan",
    "ai",
    "prompt",
    "healthcare"
  ],
  "author": "Cursorvers Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cursorvers/guidescope.git",
    "directory": "packages/core"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  }
}
